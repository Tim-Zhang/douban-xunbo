{
	"name": "迅播网豆瓣插件",
	"version": "1.3.2",
	"manifest_version": 2,
	"description": "在迅播电影页面显示豆瓣评分",
	"content_scripts": [{
		"matches": ["http://www.xiamp4.com/*"],
		"js": ["douban.js"]
	}],
	"browser_action": {
		"default_icon": "icon16.png"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"on.png","off.png"
	]
}
