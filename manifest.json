{
	"name": "XunBoDouBan",
	"version": "1.0.2",
	"manifest_version": 2,
	"description": "Allows to you request any site with ajax from any source. Adds to response 'Allow-Control-Allow-Origin: *' header",
	"content_scripts": [{
		"matches": ["http://www.xiamp4.com/*"],
		"js": ["jquery.min.js", "douban.js"]
	}],
	"browser_action": {
		"default_icon": "xunbo.png"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"on.png","off.png"
	]
}